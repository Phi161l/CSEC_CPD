def chessGame(n, s):
    anton = 0
    danik = 0
    for i in s:
        if i == "A":
            anton += 1
        else:
            danik += 1
    if anton > danik:
        return "Anton"
    elif anton < danik:
        return "Danik"
    else:
        return "Friendship"

n = int(input())
s = input()
print(chessGame(n,s))
